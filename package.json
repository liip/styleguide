{
  "name": "@liip/styleguide",
  "version": "6.2.0",
  "description": "Liip.ch official web styleguide",
  "keywords": [
    "styleguide",
    "toolkit",
    "fractal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liip/styleguide.git"
  },
  "author": "Liip AG",
  "license": "MIT",
  "homepage": "http://styleguide.liip.ch",
  "scripts": {
    "start": "NODE_ENV=development concurrently 'node ./server.js' 'webpack serve'",
    "build": "NODE_ENV=production concurrently 'npm run build:fractal' 'npm run build:webpack' && cp -R dist/* build/",
    "build:webpack": "NODE_ENV=production webpack",
    "build:fractal": "NODE_ENV=production fractal build",
    "prettier": "prettier --check assets/scripts/**/*.js",
    "prettier:fix": "prettier --write assets/scripts/**/*.js",
    "eslint": "eslint assets/scripts",
    "eslint:fix": "eslint --fix assets/scripts",
    "validate": "npm run prettier && npm run eslint",
    "format": "npm run prettier:fix && npm run eslint:fix",
    "deploy": "rsync -avzP --delete --exclude='.*' build/ liip-styleguide:/var/www/html/styleguide.liip.ch/",
    "test": "npm run validate && npm run build:webpack",
    "release": "release-it"
  },
  "main": "dist/common.js",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@frctl/fractal": "^1.5.6",
    "@frctl/nunjucks": "^2.0.10",
    "@release-it/conventional-changelog": "^2.0.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^6.0.2",
    "conventional-changelog-cli": "^2.1.1",
    "core-js": "^3.10.1",
    "css-loader": "^5.2.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-rawbot": "^1.1.1",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "mini-css-extract-plugin": "^1.4.1",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "release-it": "^14.6.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "svg-sprite-loader": "^6.0.5",
    "svgo": "^2.3.0",
    "svgo-loader": "^3.0.0",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.0.0-beta.2"
  },
  "dependencies": {
    "a11y-dialog": "^7.1.0",
    "expanding-textareas": "^1.0.2",
    "kanbasu": "^2.5.0",
    "prismjs": "^1.23.0"
  },
  "files": [
    "assets/*",
    "!assets/images/*",
    "dist/*",
    "!.DS_Store"
  ]
}
