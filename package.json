{
  "name": "liip-styleguide",
  "version": "0.1.0",
  "description": "Liip UI Toolkit",
  "license": "private",
  "private": true,
  "homepage": "http://styleguide.liip.ch",
  "scripts": {
    "build": "gulp",
    "start": "gulp --dev",
    "deploy": "rsync -avzP --exclude='.*' dist/ demo:/var/www/styleguide.liip.ch/web/",
    "lint:js": "eslint src/assets/toolkit/scripts --ignore-pattern='polyfills'",
    "lint:js:fix": "eslint --fix src/assets/toolkit/scripts --ignore-pattern='polyfills'",
    "lint:style": "csscomb -lv -c node_modules/csscomb-config-rawbot/index.json src/assets/toolkit/styles/**/*.scss",
    "lint:style:fix": "csscomb -c node_modules/csscomb-config-rawbot/index.json src/assets/toolkit/styles/**/*.scss",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "npm run lint"
  },
  "author": "Liip AG",
  "repository": "liip/styleguide",
  "engines": {
    "node": ">=4.0",
    "npm": ">=2.0.0"
  },
  "main": "dist/assets/toolkit/scripts/toolkit.js",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-env": "^1.1.5",
    "browser-sync": "^2.13.0",
    "csscomb": "^4.2.0",
    "csscomb-config-rawbot": "^0.2.0",
    "del": "^2.2.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-config-rawbot": "^0.3.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "fabricator-assemble": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-csso": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.3.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-svg-symbols": "^2.0.2",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.11",
    "imports-loader": "^0.6.5",
    "run-sequence": "^1.2.1",
    "script-loader": "^0.7.0",
    "svgxuse": "^1.2.4",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "dom-delegate": "^2.0.3",
    "ease-component": "^1.0.0",
    "es6-promise": "^4.0.5",
    "hammerjs": "^2.0.8",
    "kanbasu": "github:liip/kanbasu#53848255",
    "prismjs": "^1.6.0",
    "scroll": "^2.0.0",
    "scroll-doc": "^0.1.3",
    "whatwg-fetch": "^2.0.2"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ]
}
