{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-body-style": "off",
    "max-len": ["error", 120],
    "no-mixed-operators": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "no-plusplus": "off",
    "spaced-comment": "off"
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
